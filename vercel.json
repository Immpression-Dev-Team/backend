{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ],
    "crons": [
        {
            "path": "/orders/shipments/poll-due?secret=superlongrandomstring",
            "schedule": "0 */3 * * *"
        }
    ]
}